    "seed:languages": "npx tsx scripts/seed/core/seed-languages-comprehensive.ts",
    "seed:international": "npx tsx scripts/seed/core/seed-international-data.ts",
    "seed:install": "npx tsx scripts/seed-from-installation.ts",
    "seed:install:dry": "npx tsx scripts/seed-from-installation.ts -- --dry-run",
    "test:permissions": "NODE_ENV=development npx tsx scripts/test-permissions.ts",
    "fix:roles": "npx tsx scripts/fix-role-naming-conventions.ts",
    "fix:roles:dry": "npx tsx scripts/fix-role-naming-conventions.ts -- --dry",
    "audit:database": "npx tsx scripts/audit-database-access.ts",
    "audit:architecture": "npx tsx scripts/comprehensive-architecture-audit.ts",
    "worker:page-tracking": "tsx scripts/workers/page-tracking-worker.ts",
    "install:all": "pnpm install",
    "docker:restart": "osascript -e 'quit app \"Docker\"' && sleep 3 && open -a Docker && echo 'üê≥ Docker Desktop restarting... Please wait ~30 seconds for containers to be ready'",
    "docker:status": "docker ps --format 'table {{.Names}}\\t{{.Status}}\\t{{.Ports}}'",
    "docker:health": "docker ps && echo '\n‚úÖ Docker is running' || echo '\n‚ùå Docker is not running'",
    "docker:check": "./scripts/check-services.sh",
    "redis:connect": "./scripts/redis-connection-info.sh",
    "redis:test-data": "bash scripts/test-redis-connections.sh",
    "grafana:homepage": "./scripts/setup-grafana-homepage.sh",
    "grafana:test": "./scripts/test-grafana-dashboard.sh",
    "docker:services": "echo 'üîó Services URLs:\n‚úÖ Prometheus: http://localhost:9090\n‚úÖ Grafana: http://localhost:5005 (admin/admin123)\n‚úÖ RabbitMQ: http://localhost:15672 (admin/admin123)\n‚úÖ Mailpit: http://localhost:8025\n‚úÖ N8N: http://localhost:5678 (admin/admin123)\n‚úÖ Temporal: http://localhost:8080\n‚úÖ RedisInsight: http://localhost:5540\n‚úÖ Redis CLI: redis-cli -h localhost -p 6379\nüìä Container Metrics: http://localhost:8081'",
    "docker:fix": "pnpm run docker:restart && sleep 30 && pnpm run docker:services",
    "fix:files": "ulimit -n 10000 && echo '‚úÖ File limit increased for this session'",
    "dev:safe": "pnpm run fix:files && pnpm run dev",
    "dev:clean": "pkill -f node; pkill -f next; rm -rf .next && pnpm run dev",
    "dev:polling": "WATCHPACK_POLLING=true CHOKIDAR_USEPOLLING=true next dev --turbopack",
    "docs:migrate": "node scripts/migrate-yaml-to-markdown-simple.js"
  },
