  "name": "mono",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "scripts": {
    "dev": "turbo dev",
    "dev:api": "pnpm --filter @mono/api dev",
    "dev:web": "pnpm --filter @mono/web dev",
    "dev:all": "pnpm run dev:api & pnpm run dev:web",
    "build": "turbo build",
    "build:api": "pnpm --filter @mono/api build",
    "build:web": "pnpm --filter @mono/web build",
    "build:all": "pnpm run build:api && pnpm run build:web",
    "turbo:dev": "turbo dev --no-cache",
    "turbo:build": "turbo build --no-cache",
    "start": "pnpm --filter @mono/web start",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "seed:comprehensive": "npx tsx scripts/comprehensive-seeder.ts",
    "seed:verify": "npx tsx scripts/verify-setup.ts",
    "domains:setup": "sudo ./scripts/setup-local-domains.sh",
    "domains:cleanup": "sudo ./scripts/cleanup-local-domains.sh",
    "dev:domains": "./scripts/start-dev-domains.sh",
    "seed:tenant": "node --loader ts-node/esm scripts/seed-tenant.ts",
    "seed:auth": "npx tsx scripts/seed/core/seed-auth.ts",
    "seed:translations": "npx tsx scripts/seed/core/seed-translations.ts",
    "seed:categories-tags": "npx tsx scripts/seed-categories-tags.ts",
