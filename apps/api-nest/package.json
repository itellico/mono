{
  "name": "api-nest",
  "version": "1.0.0",
  "description": "NestJS API with Fastify adapter for itellico mono",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:html": "jest --coverage && open coverage/lcov-report/index.html",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathPattern=\"\\.spec\\.ts$\"",
    "test:unit:watch": "jest --testPathPattern=\"\\.spec\\.ts$\" --watch",
    "test:unit:cov": "jest --testPathPattern=\"\\.spec\\.ts$\" --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "test:e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-e2e.json --runInBand",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:all:cov": "npm run test:unit:cov && npm run test:e2e:cov",
    "test:clear": "jest --clearCache",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e:ci": "jest --config ./test/jest-e2e.json --ci --watchAll=false",
    "test:db:setup": "cd ../../ && pnpm prisma migrate reset --force --skip-generate && pnpm prisma migrate deploy && pnpm prisma db seed",
    "test:db:clean": "cd ../../ && pnpm prisma migrate reset --force --skip-generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^8.2.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.3",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.11.1",
    "@types/passport-local": "^1.0.38",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.8",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.0.1",
    "cache-manager-redis-store": "3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cron": "4.3.0",
    "fastify": "^4.25.2",
    "joi": "^17.13.3",
    "nestjs-pino": "^4.4.0",
    "node-cron": "^4.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/autocannon": "^7.12.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autocannon": "^8.0.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.3",
    "prettier": "^3.6.2",
    "prisma": "^6.10.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3"
  }
}
