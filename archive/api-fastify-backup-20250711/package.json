{
  "name": "@mono/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/routes",
    "test:security": "vitest run tests/security",
    "test:auth": "vitest run tests/routes/auth.test.ts",
    "test:admin": "vitest run tests/routes/admin.test.ts",
    "test:forms": "vitest run tests/routes/forms.test.ts",
    "test:health": "vitest run tests/routes/health.test.ts",
    "test:admin-suite": "vitest run tests/routes/auth.test.ts tests/routes/admin.test.ts",
    "test:setup": "bash ../../scripts/test-setup.sh up",
    "test:teardown": "bash ../../scripts/test-setup.sh down",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json}'",
    "format:write": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed:test": "tsx scripts/seed-test-data.ts",
    "temporal:worker": "tsx src/temporal/start-workers.ts",
    "temporal:worker:dev": "tsx watch src/temporal/start-workers.ts"
  },
  "dependencies": {
    "@fastify/auth": "^4.6.1",
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/etag": "^5.2.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@mono/shared": "workspace:*",
    "@prisma/client": "^6.10.1",
    "@sinclair/typebox": "^0.32.5",
    "@temporalio/activity": "^1.8.6",
    "@temporalio/client": "^1.8.6",
    "@temporalio/worker": "^1.8.6",
    "@temporalio/workflow": "^1.8.6",
    "@types/amqplib": "^0.10.7",
    "@types/bcryptjs": "^2.4.6",
    "amqplib": "^0.10.8",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.0.1",
    "fastify": "^4.25.2",
    "fastify-plugin": "^4.5.1",
    "form-data": "^4.0.3",
    "ioredis": "^5.3.2",
    "mailgun.js": "^12.0.3",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.14.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prisma": "^6.10.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}